{
  "name": "chartjs-web-components",
  "version": "2.7.0",
  "flat": true,
  "contributors": [
    "fsx950223"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && npm run lint"
    }
  },
  "main": "dist/base.js",
  "unpkg": "dist/base.js",
  "license": "MIT",
  "repository": "fsx950223/chartjs-web-components",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve --config config/development.config.ts --open",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack --config config/production.config.ts",
    "prepublish": "npm run lint && npm run build",
    "lint": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "web-components",
    "html",
    "chart.js",
    "lit-element",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chart.js": "^2.7.49",
    "@types/node": "^11.11.5",
    "@types/webpack": "^4.4.26",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "chart.js": "^2.8.0",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^28.0.2",
    "html-webpack-plugin": "^5.2.0",
    "lit-element": "2.1.0",
    "minify-lit-html-loader": "0.0.3",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^4.2.3",
    "typescript-lit-html-plugin": "^0.9.0",
    "webpack": "^5.20.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  },
  "peerDependencies": {
    "chart.js": "^2.8.0",
    "lit-element": "2.4.0"
  }
}
